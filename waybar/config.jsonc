{
  "layer": "top",
  "position": "top",
  "modules-left": ["hyprland/workspaces"],
  "modules-center": ["clock"],
  "modules-right": ["network", "battery", "volume", "cpu"],
  "clock": {
    "format": "%Y-%m-%d %H:%M:%S"
  },
  "battery": {
    "format": "{capacity}%"
  },
  "network": {
    "format": "{essid} {signal_strength}%"
  },
  "volume": {
    "format": "{volume}%"
  },
  "cpu": {
    "format": "{usage}%",
    "interval": 1
  }
}

